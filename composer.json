{
    "name": "developer/wc-commission-pricing-engine",
    "description": "WooCommerce Commission & Pricing Engine with Stripe Connect integration",
    "type": "wordpress-plugin",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Dilip",
            "email": "developer@developer.developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "stripe/stripe-php": "^13.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "^3.0",
        "phpcsstandards/phpcsextra": "^1.0",
        "phpcsstandards/phpcsutils": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WCPE\\": "src/",
            "WCPE\\Includes\\": "includes/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpcbf": "phpcbf"
    }
}
